    "lib": "lib",
    "test": "tests"
  },
  "keywords": [
    "telios",
    "decentralized",
    "distributed",
    "file",
    "sharing",
    "drive",
    "storage",
    "p2p",
    "peer-to-peer",
    "hypercore",
    "hypercore-protocol",
    "hyperdrive",
    "hyperswarm"
  ],
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
