  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.9",
    "@semantic-release/npm": "^7.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.0.1",
    "del": "^6.0.0",
    "husky": "^6.0.0",
    "npm-force-resolutions": "^0.0.10",
